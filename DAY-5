//Greek for greeks POTD --- Transport of Matrix

//User function Template for Java
class Solution
{
    public void transpose(int n,int a[][])
    {
        int b[][]  = new int[n][n];
        
        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                b[i][j] = a[i][j];
            }
        }
        
        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                a[j][i] = b[i][j];
            }
        }
    }
}
